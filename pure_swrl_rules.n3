# 规则 1: S09-0
dls:S09-0 a swrl:Rule ;
    rdfs:comment "深埋 + RockGrade I + 干燥 → 25.0 cm" ;
    swrla:isRuleEnabled true ;
    rdfs:label "S09-0" ;
    swrl:body [
        swrl:classAtom [
            swrl:class dls: ;
            swrl:variable :t
        ] ;
        swrl:objectPropertyAtom [
            swrl:objectProperty dls: ;
            swrl:variable :t ;
            swrl:namedIndividual dls:
        ] ;
        swrl:objectPropertyAtom [
            swrl:objectProperty dls: ;
            swrl:variable :gc ;
            swrl:namedIndividual dls:
        ] ;
        swrl:objectPropertyAtom [
            swrl:objectProperty dls: ;
            swrl:variable :gc ;
            swrl:namedIndividual dls:
        ] 
    ] ;
    swrl:head [
        swrl:dataPropertyAtom [
            swrl:dataProperty dls: ;
            swrl:variable :t ;
            swrl:literal "25.0"^^xsd:float
        ] 
    ].

